# mount point    fstype  device                       device2                flags
/system_root     ext4    /dev/block/platform/bootdevice/by-name/system       flags=backup=0
/system_image    emmc    /dev/block/by-name/system
/vendor          ext4    /dev/block/platform/bootdevice/by-name/vendor       flags=backup=0
/vendor_image    emmc    /dev/block/by-name/vendor
/recovery        emmc    /dev/block/platform/bootdevice/by-name/recovery
/boot            emmc    /dev/block/platform/bootdevice/by-name/boot
/cache           ext4    /dev/block/platform/bootdevice/by-name/cache
/data            ext4    /dev/block/platform/bootdevice/by-name/userdata     flags=encryptable=/dev/block/platform/bootdevice/by-name/metadata
/persistent      emmc    /dev/block/platform/bootdevice/by-name/frp
/misc            emmc    /dev/block/platform/bootdevice/by-name/para
/metadata        emmc    /dev/block/platform/bootdevice/by-name/metadata
/boot_para       emmc    /dev/block/platform/bootdevice/by-name/boot_para
/dtbo            emmc    /dev/block/platform/bootdevice/by-name/dtbo
/odmdtbo         emmc    /dev/block/platform/bootdevice/by-name/odmdtbo
/nvram           emmc    /dev/block/platform/bootdevice/by-name/nvram        flags=backup;flashimg
/nvdata          emmc    /dev/block/platform/bootdevice/by-name/nvdata       flags=backup;flashimg
/nvcfg           emmc    /dev/block/platform/bootdevice/by-name/nvcfg        flags=backup;flashimg

/external_sd     auto    /dev/block/mmcblk1p1         /dev/block/mmcblk1
/usb-otg         vfat    /dev/block/sda1              /dev/block/sda         flags=storage;removable;wipeingui
